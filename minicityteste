local cam = Window:CreateTab("Combate")

local Paragraph = cam:CreateParagraph({Title = "BIEL BS ON TOP", Content = "Feito por BIEL BS"})
local Section = cam:CreateSection("NECESS√ÅRIO")


local Button = cam:CreateButton({
	Name = "Cabe√ß√£o - Medio",
	Callback = function()


		Rayfield:Notify({
			Title = "Aviso",
			Content = "Todos Jogadores T√£o Cabe√ßudo, LA ELE!",
			Duration = 6.5,
			Image = 4483362458,
		})

		-- Fun√ß√£o para deletar todas as NotifyGui
		local function deletarNotifyGui()
			local playerGui = game.Players.LocalPlayer:WaitForChild("PlayerGui")
			for _, gui in ipairs(playerGui:GetChildren()) do
				if gui.Name == "NotifyGui" and gui:IsA("ScreenGui") then
					gui:Destroy() -- Deleta a NotifyGui
				end
			end
		end

		for _, npc in ipairs(workspace:GetChildren()) do
			local humanoid = npc:FindFirstChildOfClass("Humanoid")
			if humanoid and npc ~= game.Players.LocalPlayer.Character then
				local head = npc:FindFirstChild("Head")
				if head and head:IsA("Part") then
					head.CanCollide = false
					head.Size = Vector3.new(6, 6, 6)
					head.Transparency = .8
				end
			end
		end

	end,
})

