local Button = rev:CreateButton({
	Name = "Fugir - 50",
	Callback = function()
		local function deletarNotifyGui()
			local playerGui = game.Players.LocalPlayer:WaitForChild("PlayerGui")
			for _, gui in ipairs(playerGui:GetChildren()) do
				if gui.Name == "NotifyGui" and gui:IsA("ScreenGui") then
					gui:Destroy()
				end
			end
		end

		local TextChatService = game:GetService("TextChatService")
		local Players = game:GetService("Players")
		local LocalPlayer = Players.LocalPlayer

		local function tp()
			local TweenService = game:GetService("TweenService")
			local player = game.Players.LocalPlayer
			local character = player.Character or player.CharacterAdded:Wait()
			local humanoidRootPart = character:WaitForChild("HumanoidRootPart")

			-- Destino desejado
			local destino = humanoidRootPart.CFrame - Vector3.new(0, 10, 0)

			-- Velocidade desejada
			local VELOCIDADE = 1500  -- studs por segundo

			-- Calcular dist√¢ncia e dura√ß√£o
			local distancia = (destino - humanoidRootPart.Position).Magnitude
			local duracao = distancia / VELOCIDADE

			-- Criar TweenInfo
			local tweenInfo = TweenInfo.new(duracao, Enum.EasingStyle.Linear, Enum.EasingDirection.Out)
			local goal = {Position = destino}

			-- Ancorar antes de mover
			humanoidRootPart.Anchored = true

			-- Criar e tocar o tween
			local tween = TweenService:Create(humanoidRootPart, tweenInfo, goal)
			tween:Play()

			-- Desancorar quando terminar
			tween.Completed:Connect(function()
				humanoidRootPart.Anchored = false
			end)
		end

		while true do
			-- pequeno delay para n√£o sobrecarregar

			local character = LocalPlayer.Character
			if character then
				local humanoid = character:FindFirstChildOfClass("Humanoid")
				if humanoid and humanoid.Health < 35 then
					tp()
					break -- para o loop ap√≥s rejoin
				end
			end
			task.wait()
		end
	end,
})

local Button = rev:CreateButton({
	Name = "Anti-AFK (20 Minutos)",
	Callback = function()
		local function deletarNotifyGui()
			local playerGui = game.Players.LocalPlayer:WaitForChild("PlayerGui")
			for _, gui in ipairs(playerGui:GetChildren()) do
				if gui.Name == "NotifyGui" and gui:IsA("ScreenGui") then
					gui:Destroy()
				end
			end
		end

		local TextChatService = game:GetService("TextChatService")
		local Players = game:GetService("Players")
		local LocalPlayer = Players.LocalPlayer

		local function sendRejoinMessage()
			local channel = TextChatService:WaitForChild("TextChannels"):WaitForChild("RBXGeneral")
			channel:SendAsync("/rejoin")
		end

		-- üü¢ In√≠cio do Anti-AFK por pulo autom√°tico
		task.spawn(function()
			while true do
				Rayfield:Notify({
					Title = "MENU - MINI CITY",
					Content = "Anti-Afk, Pulei para reset time!",
					Duration = 6.5,
					Image = 4483362458,
				})

				local character = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()
				local humanoid = character:FindFirstChildOfClass("Humanoid")
				if humanoid then
					humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
				end
				wait(60) -- pula a cada 60 segundos
			end
		end)
	end,
})


